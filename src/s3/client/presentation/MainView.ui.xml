<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:AbsolutePanel>
		<g:AbsolutePanel ui:field="playground"/>
		<g:Button ui:field="btnIncreaseWidth" text="Width +"/>
		<g:Button ui:field="btnDecreaseWidth" text="Width -"/>
		<g:Button ui:field="btnDecreaseHeight" text="Height -"/>
		<g:Button ui:field="btnIncreaseHeight" text="Height +"/>
		<g:Button ui:field="btnIncreaseSpeed" text="Speed +"/>
		<g:Button ui:field="btnDecreaseSpeed" text="Speed -"/>
		<g:VerticalPanel ui:field="info">
			<g:Label text="Choose the skin:"/>
			<g:ListBox ui:field="skinsChooser"/>
		</g:VerticalPanel>
	</g:AbsolutePanel>
</ui:UiBinder> 